-- Scripts:

local function PLUNPYG_fake_script() -- Gradient.dragScript 
	local script = Instance.new('LocalScript', Gradient)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(PLUNPYG_fake_script)()
local function MYCPYO_fake_script() -- getcoinsbutton.givemoney 
	local script = Instance.new('LocalScript', getcoinsbutton)

	script.Parent.MouseButton1Click:Connect(function()
		
		-- Script generated by SimpleSpy - credits to exx#9394
	
		local args = {
			[1] = "\232\147\157\230\139\147\231\159\179",
			[2] = 1000000,
			[3] = -1
		}
	
		game:GetService("ReplicatedStorage").event.magbuy:FireServer(unpack(args))
		
	end)
end
coroutine.wrap(MYCPYO_fake_script)()
local function GMLUW_fake_script() -- Bringtreasures.btScript 
	local script = Instance.new('LocalScript', Bringtreasures)

	local qq = false
	local ww = false
	
	script.Parent.MouseButton1Click:Connect(function()
		
		if qq == false then
			qq = true
			
			if ww == false then
				script.Parent.BackgroundColor3 = Color3.new(0,255,0)
				while qq do
					wait(.1)
				
				for i,v in pairs(game:GetService("Workspace").Treasure:GetChildren()) do
	
					v.CanCollide = false
	
					v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	
					wait()
				end
				end
			end
		else
			script.Parent.BackgroundColor3 = Color3.new(255,0,0)
			qq = false
		end
		ww = false
		
	end)
end
coroutine.wrap(GMLUW_fake_script)()
local function KPLTCY_fake_script() -- killallenemiesinow.koweScript 
	local script = Instance.new('LocalScript', killallenemiesinow)

	local ff = false
	local gg = false
	
	script.Parent.MouseButton1Click:Connect(function()
		
		if ff == false then
			ff= true
			
			if gg == false then
				
				script.Parent.BackgroundColor3 = Color3.new(0,255,0)
				while ff do
					wait(.3)
					for i,v in pairs(game:GetService("Workspace").enemy:GetChildren()) do
	
						game:GetService("ReplicatedStorage").event.attack:FireServer(v,math.huge,"\230\149\140\228\186\186")
	
					end
				end
				
			end
		else
			script.Parent.BackgroundColor3 = Color3.new(255,0,0)
			ff = false
		end
		
		gg = false
		
	end)
end
coroutine.wrap(KPLTCY_fake_script)()
